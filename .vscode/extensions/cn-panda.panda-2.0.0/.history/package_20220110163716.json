{
  "name": "panda-vscode",
  "publisher": "Panda",
  "displayName": "Panda",
  "description": "Panda is a free IDE extension that lets you fix globalization bugs as you write code! Across popular IDEs (IntelliJ, VS Code) and popular programming languages, Panda helps all developers write better code!",
  "version": "1.0.9",
  "icon": "static/icon/log.png",
  "engines": {
    "vscode": "^1.52.0"
  },
  "repository": "git@gitlab.alibaba-inc.com:g11n-venus/mds-i18n-vscode.git",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:venus.ignoreFile",
    "onCommand:venus.checkPage",
    "onCommand:venus.checkFile",
    "onCommand:venus.ignoreFileFolder",
    "onView:venus.textView",
    "onCommand:venus.clickItem"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "venus.checkPage",
        "title": "使用 Panda 扫描文案",
        "icon": "static/icon/check.svg"
      },
      {
        "command": "venus.checkFile",
        "title": "使用 Panda 扫描文件夹"
      },
      {
        "command": "venus.ignoreFile",
        "title": "使用 Panda 跳过对此文件的扫描"
      },
      {
        "command": "venus.ignoreFileFolder",
        "title": "使用 Panda 跳过对此文件夹的扫描"
      },
      {
        "command": "venus.textView",
        "title": "打开扫描结果"
      },
      {
        "command": "venus.exportView",
        "title": "导出已修复到美杜莎"
      },
      {
        "command": "venus.refreshView",
        "title": "更新扫描结果"
      },
      {
        "command": "venus.fixView",
        "title": "修复全部"
      },
      {
        "command": "venus.fixFile",
        "title": "修复当前文件/夹"
      },
      {
        "command": "venus.fixItem",
        "title": "修复"
      },
      {
        "command": "venus.ignoreItem",
        "title": "忽略"
      },
      {
        "command": "venus.clickItem",
        "title": "选择某个命令"
      },
      {
        "command": "venus.checkMedusaText",
        "title": "复用美杜莎的key"
      },
      {
        "command": "venus.publishPack",
        "title": "发布已修复到日常语言包"
      }
    ],

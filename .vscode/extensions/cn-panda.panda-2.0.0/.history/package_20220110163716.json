    "menus": {
      "editor/title": [
        {
          "when": "workspaceFolderCount >= 1 && ( resourceLangId == asa || resourceLangId == asax || resourceLangId == ascx || resourceLangId == ashx || resourceLangId == asmx || resourceLangId == asp || resourceLangId == aspx || resourceLangId == axd || resourceLangId == cshtml || resourceLangId == ejs || resourceLangId == htm || resourceLangId == html || resourceLangId == inc || resourceLangId == jsp || resourceLangId == jspf || resourceLangId == jspx || resourceLangId == mas || resourceLangId == master || resourceLangId == mi || resourceLangId == php || resourceLangId == shtml || resourceLangId == skin || resourceLangId == tag || resourceLangId == vm || resourceLangId == xhtml || resourceLangId == as || resourceLangId == javascriptreact || resourceLangId == javascript  || resourceLangId == typescriptreact )",
          "command": "venus.checkPage",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "filesExplorerFocus && explorerResourceIsFolder && workspaceFolderCount >= 1 ",
          "command": "venus.checkFile",
          "group": "2_workspace"
        },
        {
          "when": "filesExplorerFocus && explorerResourceIsFolder && workspaceFolderCount >= 1 ",
          "command": "venus.ignoreFileFolder",
          "group": "2_workspace"
        },
        {
          "when": "resourceLangId == asa || resourceLangId == asax || resourceLangId == ascx || resourceLangId == ashx || resourceLangId == asmx || resourceLangId == asp || resourceLangId == aspx || resourceLangId == axd || resourceLangId == cshtml || resourceLangId == ejs || resourceLangId == htm || resourceLangId == html || resourceLangId == inc || resourceLangId == jsp || resourceLangId == jspf || resourceLangId == jspx || resourceLangId == mas || resourceLangId == master || resourceLangId == mi || resourceLangId == php || resourceLangId == shtml || resourceLangId == skin || resourceLangId == tag || resourceLangId == vm || resourceLangId == xhtml || resourceLangId == as || resourceLangId == javascriptreact || resourceLangId == javascript || resourceLangId == typescriptreact",
          "command": "venus.checkPage",
          "group": "2_workspace"
        },
        {
          "when": "resourceLangId == asa || resourceLangId == asax || resourceLangId == ascx || resourceLangId == ashx || resourceLangId == asmx || resourceLangId == asp || resourceLangId == aspx || resourceLangId == axd || resourceLangId == cshtml || resourceLangId == ejs || resourceLangId == htm || resourceLangId == html || resourceLangId == inc || resourceLangId == jsp || resourceLangId == jspf || resourceLangId == jspx || resourceLangId == mas || resourceLangId == master || resourceLangId == mi || resourceLangId == php || resourceLangId == shtml || resourceLangId == skin || resourceLangId == tag || resourceLangId == vm || resourceLangId == xhtml || resourceLangId == as || resourceLangId == javascriptreact || resourceLangId == javascript || resourceLangId == typescriptreact",
          "command": "venus.ignoreFile",
          "group": "2_workspace"
        }
      ],
      "view/title": [
        {
          "command": "venus.fixView",
          "group": "navigation",
          "when": "view == venus.textView"
        },
        {
          "command": "venus.refreshView",
          "group": "navigation",
          "when": "view == venus.textView"
        }
      ],
      "view/item/context": [
        {
          "command": "venus.fixFile",
          "group": "inline",
          "when": "viewItem == file "
        },
        {
          "command": "venus.fixItem",
          "group": "inline",
          "when": "viewItem == page "
        },
        {
          "command": "venus.publishPack",
          "group": "inline",
          "when": "viewItem == viewTitle "
        },
        {
          "command": "venus.ignoreItem",
          "group": "inline",
          "when": "viewItem == page "
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Venus",
      "properties": {
        "mds.i18n.mcms.appName": {
          "type": "string",
          "markdownDescription": "美杜莎应用名"
        },
        "mds.i18n.user.accessKey": {
          "type": "string",
          "markdownDescription": "BUC Id"
        },
        "mds.i18n.rule.JavaciptRuleType": {
          "type": "string",
          "default": ":icbu:javascript-ext.venus",
          "enum": [
            "None",
            "JavaScript Rule Set",
            "JavaScript CJK only",
            "JavaScript CJK New",
            ":dingtalk:javascript-ext.DingFlowBpms",
            ":tmall:javascript-ext.dbank",
            ":alicloud:javascript-ext.cjk",
            ":alicloud:javascript-ext.cloud",
            ":alicloud:javascript-ext.ecs",
            ":alicloud:javascript-ext.splc",
            ":icbu:javascript-ext.ggs",
            ":icbu:javascript-ext.magellan",
            ":icbu:javascript-ext.venus",
            ":ae:javascript-ext.pc",
            ":cainiao:javascript-ext.cainiao",
            ":cainiao:javascript-ext.wmp",
            ":sirius:javascript-ext.sirius",
            ":antibg:javascript-ext.antibg",
            ":antibg:javascript-ext.iam",
            ":antpay:javascript-ext.antpay",
            ":bp-xt:javascript-ext.arms",
            ":lzd:javascript-ext.lazgame"
          ],
          "enumDescriptions": [
            "None",
            "JavaScript",
            "JavaScriptCJK",
            "JSCJK",
            ":dingtalk:javascript-ext.DingFlowBpms",
            ":tmall:javascript-ext.dbank",
            ":alicloud:javascript-ext.cjk",
            ":alicloud:javascript-ext.cloud",
            ":alicloud:javascript-ext.ecs",
            ":alicloud:javascript-ext.splc",
            ":icbu:javascript-ext.ggs",
            ":icbu:javascript-ext.magellan",
            ":icbu:javascript-ext.venus",
            ":ae:javascript-ext.pc",
            ":cainiao:javascript-ext.cainiao",
            ":cainiao:javascript-ext.wmp",
            ":sirius:javascript-ext.sirius",
            ":antibg:javascript-ext.antibg",
            ":antibg:javascript-ext.iam",
            ":antpay:javascript-ext.antpay",
            ":bp-xt:javascript-ext.arms",
            ":lzd:javascript-ext.lazgame"
          ],
          "description": "Javascript Rule"
        },
        "mds.i18n.rule.HtmlRuleType": {
          "type": "string",
          "default": ":icbu:html-ext.venus",
          "enum": [
            "None",
            "HTML",
            "HtmlCJK",
            ":dingtalk:html-ext.DingFlowBpms",
            ":tmall:html-ext.dbank",
            ":icbu:html-ext.ggs",
            ":icbu:html-ext.magellan",
            ":icbu:html-ext.venus",
            ":ae:html-ext.pc",
            ":alicloud:html-ext.cloud",
            ":alicloud:html-ext.splc",
            ":cainiao:html-ext.cainiao",
            ":sirius:html-ext.sirius",
            ":antibg:html-ext.antibg",
            ":antpay:html-ext.antpay"
          ],
          "enumDescriptions": [
            "None",
            "HTML Rule Set",
            "HTML CJK only",
            ":dingtalk:html-ext.DingFlowBpms",
            ":tmall:html-ext.dbank",
            ":icbu:html-ext.ggs",
            ":icbu:html-ext.magellan",
            ":icbu:html-ext.venus",
            ":ae:html-ext.pc",
            ":alicloud:html-ext.cloud",
            ":alicloud:html-ext.splc",
            ":cainiao:html-ext.cainiao",
            ":sirius:html-ext.sirius",
            ":antibg:html-ext.antibg",
            ":antpay:html-ext.antpay"
          ],
          "description": "Html Rule"
        },
        "mds.i18n.rule.fileExclusions": {
          "description": "Files to exclude",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "mds.i18n.rule.server": {
          "type": "number",
          "default": 1,
          "enum": [
            0,
            1,
            2,
            3
          ],
          "enumDescriptions": [
            "错误(Error)",
            "警告(Warning)",
            "信息(Information)",
            "提示(Hint)"
          ],
          "description": "扫描完成后展示结果的等级"
        },
        "mds.i18n.text.language": {
          "type": "string",
          "default": "zh_CN",
          "enum": [
            "zh_CN",
            "en_US"
          ],
          "enumDescriptions": [
            "中文",
            "英文"
          ],
          "description": "代码中硬编码文案的语种"
        },
        "mds.i18n.mcms.key": {
          "type": "string",
          "markdownDescription": "#### 替换时使用key的格式 \n- `groupName`: git 组名称，比如`venus`(可选) \n- `projectName`: git项目名称，比如`venus-vscode`(可选)\n- `filePath`: 文件路径,比如`src.pages.test.index`(可选)\n- `value`: 文案，翻译成英文后取前三个单词，比如`This.is.Text`(必选)\n-  推荐的key格式:``` {{value}} ``` \n- 如果想要组合，比如组合 filePath 和 value，可以写成 ```{{filePath}}.{{value}}```",
          "default": "{{value}}"
        },
        "mds.i18n.mcms.keySuffix": {
          "type": "string",
          "markdownDescription": "key的后缀，一般用于对词进行分类，比如按照业务域来对词条进行打标（可不填）"
        },
        "mds.i18n.mcms.method": {
          "type": "string",
          "markdownDescription": "#### 获取文案的代码格式\n - `key`: 美杜莎平台中文案的key\n- `defaultMessage`: 初始的文案",
          "default": "$i18n.get({id:'$key$',dm:'$defaultMessage$'})"
        },
        "mds.i18n.mcms.util": {
          "type": "string",
          "markdownDescription": "i18n工具包",
          "default": "import $i18n from '@alife/panda-i18n';"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "venus.textView",
          "name": "PANDA",
          "icon": "static/icon/log.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build:watch": "webpack --watch --progress",
    "build": "webpack --progress",
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css out/",
    "test": "node ./out/test/runTest.js"
  },
